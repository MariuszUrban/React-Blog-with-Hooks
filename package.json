{
  "homepage": "https://github.com/MariuszUrban/React-Blog-with-Hooks.git",
  "name": "react-blog-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@rehooks/window-size": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "@use-hooks/axios": "^1.2.0",
    "axios": "^0.18.0",
    "babel-eslint": "10.x",
    "concurrently": "^5.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "http-proxy-middleware": "^0.19.1",
    "json-server": "^0.16.3",
    "navi": "^0.12.7",
    "react": "^16.9.0",
    "react-busy-indicator": "^1.0.0",
    "react-dom": "^16.9.0",
    "react-hookedup": "^0.1.1",
    "react-navi": "^0.12.7",
    "react-request-hook": "^2.0.1",
    "react-scripts": "^4.0.2",
    "use-debounce": "^2.1.0",
    "use-undo": "^1.0.0"
  },
  "scripts": {
    "lint": "npx eslint src/",
    "start": "npx concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "npx json-server --watch server/db.json --port 4000 --routes server/routes.json",
    "start:client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:nowatch": "npx concurrently --kill-others --success \"first\" \"npm run start:server\" \"npm run test:client -- --watchAll=false\"",
    "test:client": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/react-hooks": "^1.0.4",
    "eslint-plugin-react-hooks": "2.x",
    "gh-pages": "^3.1.0",
    "react-test-renderer": "^16.9.0"
  }
}
